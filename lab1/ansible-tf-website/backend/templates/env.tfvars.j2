gcp_project_id = "{{ gcp_project_id }}"
gcp_region = "{{ gcp_region }}"

machine_type = "{{ machine_type | default("e2-micro") }}"
machine_zone = "{{ machine_zone | default("asia-southeast1-a") }}"
initialize_image = "{{ initialize_image | default("debian-cloud/debian-11") }}"
owner_name = "{{ ansible_user }}"

{% if labels is defined %}
default_labels = {
{% for key, value in labels.items() %}
  {{ key }} = "{{ value }}"
{% endfor %}
}
{% endif %}